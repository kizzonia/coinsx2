<p id="notice"><%= notice %></p>

<h1>Tradeservices</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Slug</th>
      <th>Icon</th>
      <th>Sub title</th>
      <th>Body</th>
      <th>Tserviceimg</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tradeservices.each do |tradeservice| %>
      <tr>
        <td><%= tradeservice.title %></td>
        <td><%= tradeservice.slug %></td>
        <td><%= tradeservice.icon %></td>
        <td><%= tradeservice.sub_title %></td>
        <td><%= tradeservice.body %></td>
        <td><%= tradeservice.tserviceimg %></td>
        <td><%= link_to 'Show', tradeservice %></td>
        <td><%= link_to 'Edit', edit_tradeservice_path(tradeservice) %></td>
        <td><%= link_to 'Destroy', tradeservice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tradeservice', new_tradeservice_path %>
